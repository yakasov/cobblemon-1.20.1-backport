{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:ember_impactlinger",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/ember"
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 40,
				"max_particles": 20
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.5
			},
			"minecraft:emitter_shape_sphere": {
				"radius": "q.entity_radius*(0.65+v.emitter_age)",
				"surface_only": true,
				"direction": ["math.random(-1.5,1.5)", "math.random(0,2)", "math.random(-1.5,1.5)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.25
			},
			"minecraft:particle_initial_speed": "math.random(2.75,4.75)*0.45",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, 6, 0],
				"linear_drag_coefficient": 2
			},
			"minecraft:particle_appearance_billboard": {
				"size": [0.12, 0.12],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 25,
					"texture_height": 5,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [5, 5],
						"step_UV": [5, 0],
						"frames_per_second": 20,
						"max_frame": 5
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#FFFFC8B3",
						"1.0": "#FFB23000"
					}
				}
			}
		}
	}
}
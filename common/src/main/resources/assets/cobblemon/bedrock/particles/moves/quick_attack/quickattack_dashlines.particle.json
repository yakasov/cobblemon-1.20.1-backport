{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:quickattack_dashlines",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particles/quickattack_dashlines"
			}
		},
		"curves": {
			"variable.qsquish": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 1, 1, 0]
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 100,
				"max_particles": 100
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.25
			},
			"minecraft:emitter_shape_sphere": {
				"offset": [0, 0, -1],
				"radius": "math.clamp(query.entity_radius,1,50)",
				"direction": [0, 0, 1]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.2
			},
			"minecraft:particle_initial_speed": "9+v.particle_age",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, 0, 1]
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.2*v.qsquish", 0.02],
				"facing_camera_mode": "lookat_direction",
				"uv": {
					"texture_width": 14,
					"texture_height": 1,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [2, 1],
						"step_UV": [2, 0],
						"frames_per_second": 16,
						"max_frame": 7
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.17": "#FFB4FEFF",
						"0.5": "#ffffffff",
						"0.86": "#FFB7FEFF"
					}
				}
			}
		}
	}
}
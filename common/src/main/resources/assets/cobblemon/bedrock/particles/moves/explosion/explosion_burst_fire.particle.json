{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:explosion_burst_fire",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "cobblemon:textures/particle/impact-fire"
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 64
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": "2/24"
			},
			"minecraft:emitter_shape_point": {},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "math.random(12,24)/24 "
			},
			"minecraft:particle_initial_spin": {
				"rotation": "90*math.random_integer(1,4)"
			},
			"minecraft:particle_initial_speed": "v.particle_random_3*10 +10",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, "5 + 30*v.particle_random_1", 0],
				"linear_drag_coefficient": "2.5 + 5*v.particle_random_2"
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["math.max(0.8 -v.particle_age*(0.5+v.particle_random_1), 0)", "math.max(0.8 -v.particle_age*(0.5+v.particle_random_1), 0)"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 8,
					"texture_height": 56,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [8, 8],
						"step_UV": [0, 8],
						"frames_per_second": 24,
						"max_frame": 7,
						"stretch_to_lifetime": true
					}
				}
			},
			"minecraft:particle_motion_collision": {
				"coefficient_of_restitution": 0.5,
				"collision_radius": 0.1
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#ffffffff",
						"0.56": "#ffffffff",
						"1.0": "#00FFFFFF"
					}
				}
			}
		}
	}
}
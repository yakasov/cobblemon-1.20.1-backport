{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:sweet_scent_puffs",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particles/pink_obscure"
			}
		},
		"curves": {
			"variable.size": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 1]
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 24
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1
			},
			"minecraft:emitter_shape_disc": {
				"radius": 1,
				"surface_only": true,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "math.random(1.5,3)"
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random(1,4) * 90"
			},
			"minecraft:particle_initial_speed": "4 * q.entity_size",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, 2, 0],
				"linear_drag_coefficient": "2 * q.entity_size"
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["2 * v.size", "2* v.size"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 16,
					"texture_height": 16,
					"uv": [0, 0],
					"uv_size": [16, 16]
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#42FFFFFF",
						"1.0": "#00FFFFFF"
					}
				}
			}
		}
	}
}
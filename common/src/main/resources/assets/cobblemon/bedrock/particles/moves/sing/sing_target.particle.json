{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:sing_target",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particles/note"
			}
		},
		"curves": {
			"variable.songsize": {
				"type": "linear",
				"input": "v.particle_age",

			},
			"minecraft:emitter_lifetime_looping": {
				"active_time": 3
			},
			"minecraft:emitter_shape_sphere": {
				"offset": [0, 0.2, 0],
				"radius": "q.entity_radius*1.1",
				"surface_only": true,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 1.75
			},
			"minecraft:particle_initial_speed": "math.random(1.31,1.325)*0.3",
